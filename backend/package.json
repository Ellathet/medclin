{
  "name": "medclin-backend",
  "version": "0.0.1",
  "description": "A simple medic app",
  "main": "src/server.ts",
  "author": "Thet",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "start:dev": "ts-node-dev --ignore-watch node_modules -r tsconfig-paths/register src/server.ts",
    "seed:db": "ts-node prisma/seeds/seed.ts",
    "update:db": "yarn prisma migrate dev && yarn seed:db && yarn prisma generate",
    "docker:up": "cd docker && docker-compose -p medclin up",
    "docker:down": "cd docker && docker-compose -p medclin down"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/luxon": "^2.3.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.13",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "prisma": "^3.8.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "argon2": "^0.28.4",
    "console-stamp": "^3.0.4",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  }
}
